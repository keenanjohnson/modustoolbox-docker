IDI_ICON1 ICON DISCARDABLE "${TOOL_ICON}"
#include <windows.h>

VS_VERSION_INFO VERSIONINFO
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904E4"
        BEGIN
            VALUE "ProductName",        "@DISPLAY_NAME@"
            VALUE "ProductVersion",     "@PROJECT_VERSION_MAJOR@.@PROJECT_VERSION_MINOR@.@PROJECT_VERSION_PATCH@"
            VALUE "LegalCopyright",     "(c) @YEAR@, Cypress Semiconductor Corporation (an Infineon company) or an affiliate of Cypress Semiconductor Corporation"
        END
    END

    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1252
    END
END